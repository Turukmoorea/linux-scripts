# ================================================================================================
# = CNAME RECORDS ================================================================================
www.example.com.        IN      CNAME         example.com.
cname.no-ttl.com.               CNAME         example.com.
cname.trailing-dot.com. 1800    IN    CNAME   example.com.
cname.fqdn             1800    IN    CNAME   fqdn.example.com.

ttl="86400"
tsig_key_file="/home/timon/tsig_ftec2.key"

# ================================================================================================
# = MX RECORDS ===================================================================================
example.com.            3600    IN    MX      10 mail1.example.com.
example.com.            3600    IN    MX      20 mail2.example.com.
mx.no-ttl.com.                  IN    MX      10 mail3.example.com.
mx.invalid-priority.com. 3600   IN    MX      notanumber mail.example.com.

# ================================================================================================
# = TXT RECORDS ==================================================================================
example.com.            3600    IN    TXT     "v=spf1 include:_spf.example.net ~all"
example.com.            3600    IN    TXT     "singlequoted"     ; Quotes should stay
example.com.            3600    IN    TXT     "multiple words  with  extra   spaces"
txt.no-quotes.com.      3600    IN    TXT     v=spf1 include:_spf.example.net ~all   ; Not quoted!

# ================================================================================================
# = SRV RECORDS ==================================================================================
_service._tcp.example.com.  3600 IN SRV  10 20 8080 srv.example.com.
_service._udp.example.com.  3600 IN SRV  5  10 5060 sip.example.com.
srv.no-ttl.com.                   IN SRV  0 0 443 service.example.com.

# ================================================================================================
# = TLSA RECORDS =================================================================================
_443._tcp.example.com.     3600    IN    TLSA   3 1 1 abcd1234ef5678
tlsa.no-ttl.com.                  IN    TLSA   2 1 1 deadbeefcafebabe

zone="sandboxed.ch."

# ================================================================================================
# = CAA RECORDS ==================================================================================
example.com.            3600    IN    CAA     0 issue "letsencrypt.org"
example.com.            3600    IN    CAA     0 issuewild "comodoca.com"
example.com.            3600    IN    CAA     0 iodef "mailto:admin@example.com"
caa.no-quotes.com.      3600    IN    CAA     0 issue letsencrypt.org    ; Should remain quoted!

# ================================================================================================
# = UNSUPPORTED RECORDS ==========================================================================
unsupported.example.com. 3600    IN    NS      ns1.example.com.
unsupported.example.com. 3600    IN    SOA     ns1.example.com. hostmaster.example.com. 1 7200 3600 1209600 3600

# ================================================================================================
# = MALFORMED ====================================================================================
malformed1.example.com.  invalidttl    IN A   10.0.0.2
malformed2.example.com.  3600          HS A   10.0.0.3    ; Unsupported class HS
malformed3.example.com.  3600          IN AAAA not_an_ipv6
malformed4.example.com.  3600                 BOGUS 10.0.0.4

# ================================================================================================


